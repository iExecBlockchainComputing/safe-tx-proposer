{
  "name": "safe-tx-proposer",
  "version": "1.0.0",
  "description": "Safe Multisig Transaction Proposer for EVM Networks",
  "main": "dist/safe/index.js",
  "types": "dist/safe/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "node dist/safe/transaction-executor.js",
    "lint": "eslint safe/**/*.ts",
    "lint:fix": "eslint safe/**/*.ts --fix",
    "clean": "rimraf dist",
    "safe:transaction-lister": "ts-node safe/cli/transaction-lister.cli.ts",
    "safe:execute": "ts-node safe/cli/transaction-executor.cli.ts",
    "safe:validate": "ts-node -e \"import('./safe/validation').then(v => v.Validator.validateAll())\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "safe",
    "multisig",
    "ethereum",
    "blockchain",
    "iexec",
    "typescript", 
    "foundry",
    "transaction-automation"
  ],
  "author": "iExec Blockchain Computing",
  "license": "MIT",
  "dependencies": {
    "@safe-global/api-kit": "^4.0.0",
    "@safe-global/protocol-kit": "^6.1.0",
    "@safe-global/types-kit": "^3.0.0",
    "dotenv": "^16.4.5",
    "ethers": "^6.13.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iExecBlockchainComputing/safe-tx-proposer.git"
  },
  "files": [
    "dist/**/*",
    "safe/**/*",
    ".env.template",
    "README.md"
  ]
}
